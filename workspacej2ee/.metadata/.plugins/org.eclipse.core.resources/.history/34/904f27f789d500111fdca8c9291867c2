

import java.net.*;
import java.io.*;

public class SocketServeur {
	public static void main(String[] args)
	{
		ServerSocket s;
		try
		{
			s = new ServerSocket(80);
			while(true)
			{
				Socket soc = s.accept();
				SocketService ss=new SocketService(soc);
				ss.start();
			}
		} catch (IOException e) {e.printStackTrace();}
	}
}