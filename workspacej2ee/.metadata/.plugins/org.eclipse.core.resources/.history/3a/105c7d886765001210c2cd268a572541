package Model;

public class File {
	
	//Get the extension in the name of a file without the '.' (ex : 'Shrek.avi' become 'avi')
	public static String getExtension(String fileName){
		if(fileName.lastIndexOf(".") != -1) {
			return fileName.substring(fileName.lastIndexOf(".") + 1);
		} else {
			return "";
		}
		
	}
}